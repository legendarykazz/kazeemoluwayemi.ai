    {
      "parameters": {
        "jsCode": "const html = $input.item.json.data || $input.item.json.body || '';\n\nconst emailRegex = /[a-zA-Z0-9._%+-]+@(gmail\\.com|yahoo\\.com|hotmail\\.com)/gi;\n\nconst emails = html.match(emailRegex) || [];\n\nconst uniqueEmails = [...new Set(emails.map(email => email.toLowerCase()))];\n\nconst currentUrl = $('Loop Over URLs').item.json.urls || '';\n\nreturn uniqueEmails.map(email => ({\n  json: {\n    email,\n    sourceUrl: currentUrl,\n    extractedAt: new Date().toISOString()\n  }\n}));"
      },
